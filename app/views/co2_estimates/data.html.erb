<h1>Looking at the Data... How Green are these States?</h1>

<div class="row">
  <div class="col-md-6">
    <h3>Washington</h3>
    <p>Average Emittance: <%= @washington.average_rating %> lbs CO<sub>2</sub>/mo</p>
    <table class="table">
      <thead>
        <tr>
          <th>monthly emittance</th>
        </tr>
      </thead>
      <tbody>
        <% @washington.each do |est| %>
          <tr>
            <td><%= est.monthly_emittance %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>


  <div class="col-md-6">
    <h3>Oregon</h3>
      <p>Average Emittance: <%= @oregon.average_rating %> lbs CO<sub>2</sub>/mo</p>
    <table class="table">
      <thead>
        <tr>
          <th>monthly emittance</th>
        </tr>
      </thead>
      <tbody>
        <% @oregon.each do |est| %>
          <tr>
            <td><%= est.monthly_emittance %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

</div>
